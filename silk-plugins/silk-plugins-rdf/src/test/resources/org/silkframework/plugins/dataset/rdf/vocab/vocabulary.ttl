@prefix :     <https://silkframework.org/testOntology/> .
@prefix dct:  <http://purl.org/dc/terms/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix owl:  <http://www.w3.org/2002/07/owl#> .
@prefix rdf:  <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd:  <http://www.w3.org/2001/XMLSchema#> .
@prefix void: <http://rdfs.org/ns/void#> .
@prefix vann: <http://purl.org/vocab/vann/> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#>

: a owl:Ontology ;
  rdfs:label "Test Ontology"@en ;
.

:Movie
  a owl:Class ;
  rdfs:label "Movie"@en ;
  rdfs:isDefinedBy : ;
.

:Person
  a rdfs:Class ;
  rdfs:label "Person"@en ;
  skos:definition "A Person"@en ;
  rdfs:isDefinedBy : ;
.

:hasDate
  a owl:DatatypeProperty ;
  rdfs:label "release date"@en ;
  rdfs:domain :Movie ;
#  rdfs:range xsd:string ;
  rdfs:isDefinedBy : ;
.

:hasDirector
   a owl:ObjectProperty ;
   rdfs:label "director"@en ;
   rdfs:comment "Director of a movie"@en ;
   rdfs:domain :Movie ;
   rdfs:range :Person ;
   rdfs:isDefinedBy : ;
.

:Employee
  a owl:Class ;
  rdfs:label "employee"@en ;
  rdfs:label "Angestellter"@de ;
  rdfs:comment "Employee of a company"@en ;
  rdfs:comment "Angestellter einer Firma"@de ;
  rdfs:subClassOf :Person ;
  rdfs:isDefinedBy : ;
.

_:BlankNodeProperty
   a owl:ObjectProperty ;
   rdfs:label "Ignore me"@en ;
   rdfs:isDefinedBy : ;
.

_:BlankNodeClass
  a owl:Class ;
  rdfs:label "Ignore me"@en ;
  rdfs:isDefinedBy : ;
.
